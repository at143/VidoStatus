<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#363434" /> <!-- Background color for the progress bar -->
            <corners android:radius="35dp" /> <!-- Radius for rounded corners -->
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#3F51B5" /> <!-- Green color for the progress -->
                <corners android:radius="35dp" /> <!-- Radius for rounded corners -->
            </shape>
        </clip>
    </item>
</layer-list>
